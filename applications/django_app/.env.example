# Django settings
DJANGO_SETTINGS_MODULE=PROJECT_NAME.settings
SECRET_KEY=your-very-secret-key
DEBUG=0
ALLOWED_HOSTS=your.domain.com,localhost

# Database (example for PostgreSQL)
POSTGRES_DB=PLACEHOLDER_DB_NAME
POSTGRES_USER=PLACEHOLDER_DB_USER
POSTGRES_PASSWORD=PLACEHOLDER_DB_PASS
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Redis (for Celery broker)
REDIS_PASSWORD=your_redis_password
CELERY_BROKER_URL=redis://:your_redis_password@redis:6379/0

# (Optional) Email, cache, or other Django environment variables can go here